# CoinGecko API Configuration
# Get your API key from https://www.coingecko.com/api/pricing
VITE_COINGECKO_API_KEY=your_coingecko_api_key_here

# CryptoCompare API Configuration
# Get your API key from https://www.cryptocompare.com/cryptopian/api-keys
VITE_CRYPTOCOMPARE_API_KEY=your_cryptocompare_api_key_here

# OpenAI API Key (optional, for AI-powered changelog generation)
OPENAI_API_KEY=your_openai_api_key_here

# Cloudflare R2 credentials for update service
# Get your credentials from Cloudflare R2 dashboard
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
R2_PUBLIC_URL=https://pub-your_account_id.r2.dev
R2_BUCKET_NAME=cryptoconverter-downloads

# API Configuration
API_BASE_URL=https://cryptovertx.com/api

# Note: All custom environment variables for the frontend must be prefixed with VITE_ to be accessible
